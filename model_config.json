{
	"data_columns": ["Close","Volume"],
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"layers": [
			{
				"type": "lstm",
				"neurons": 256,
				"return_seq": true,
				"dropout_rate": 0.05,
                "input_timesteps": 60,
                "input_dim": 2
            },
			{
				"type": "dropout",
				"dropout_rate": 0.05
			},
			{
				"type": "lstm",
				"neurons": 256,
				"return_seq": false,
				"dropout_rate": 0.05
            },
			{
				"type": "dropout",
				"dropout_rate": 0.05
			},
			{
				"type": "dense",
				"neurons": 16,
				"activation": "relu"
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
